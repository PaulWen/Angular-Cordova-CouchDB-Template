{
  "name": "angular2-seed",
  "version": "0.1.0",
  "description": "AngularJS 2 project template",
  "scripts": {
    "check-for-updates": "npm outdated",
    "update": "npm update -dev --save",
    "preinstall": "npm config set python python2.7 && npm config set msvs_version 2015 && npm install cordova -g",
    "postinstall": "typings install && npm run config-couchdb-cors && npm run check-for-updates",
    "pull": "git fetch --all",
    "push": "git push --all origin",
    "config-couchdb-cors": "npm install add-cors-to-couchdb && add-cors-to-couchdb"
  },
  "keywords": [],
  "author": "Paul Wenzel <wenzel.pgb@googlemail.com>",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "*",
    "@angular/compiler": "*",
    "@angular/core": "*",
    "@angular/forms": "*",
    "@angular/http": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "@angular/router": "*",
    "angular-sortablejs": "*",
    "body-parser": "*",
    "es6-shim": "*",
    "express": "*",
    "jquery-param": "*",
    "material-design-lite": "*",
    "pouchdb": "*",
    "reflect-metadata": "*",
    "rxjs": "*",
    "sortablejs": "*",
    "superlogin": "*",
    "systemjs": "*",
    "zone.js": "*"
  },
  "devDependencies": {
    "browser-sync": "^2.12.8",
    "del": "^1.1.1",
    "fs": "0.0.2",
    "gulp": "^3.8.11",
    "gulp-develop-server": "^0.5.2",
    "gulp-html-replace": "^1.5.5",
    "gulp-remove-code": "^1.0.2",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.1",
    "gulp-sequence": "^0.4.5",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^3.1.3",
    "systemjs-builder": "^0.15.31",
    "taco-team-build": "^0.2.2",
    "typescript": "^2.1.1",
    "typings": "^2.0.0"
  }
}
